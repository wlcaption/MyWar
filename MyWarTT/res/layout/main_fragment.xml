<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <Button
        android:id="@+id/main_logout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/main_logout"
        android:textSize="18sp"/>
    <Button
        android:id="@+id/main_pay"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/main_pay"
        android:textSize="18sp"/>
    <Button
        android:id="@+id/main_showFloatView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/main_showflowtview"
        android:textSize="18sp"/>
    <Button
        android:id="@+id/main_hidFloatView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/main_hideflowtview"
        android:textSize="18sp"/>
</LinearLayout>